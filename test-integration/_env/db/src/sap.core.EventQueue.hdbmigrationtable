== version=1
COLUMN TABLE sap_core_EventQueue (
  ID NVARCHAR(36) NOT NULL,
  type NVARCHAR(5000) NOT NULL,
  subType NVARCHAR(5000) NOT NULL,
  referenceEntity NVARCHAR(5000),
  referenceEntityKey NVARCHAR(36),
  status INTEGER NOT NULL DEFAULT 0,
  payload NCLOB,
  attempts INTEGER NOT NULL DEFAULT 0,
  lastAttemptTimestamp TIMESTAMP,
  createdAt TIMESTAMP,
  PRIMARY KEY(ID)
)
